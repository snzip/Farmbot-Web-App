{
  "name": "farmbot-web-frontend",
  "version": "1.1.0",
  "description": "Farmbot web frontend.",
  "main": "dist/entry.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/farmbot/farmbot-web-frontend"
  },
  "scripts": {
    "coverage": "cat **/*lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "TARGET=production bundle exec rake webpack:compile",
    "start": "echo '===We use `npm run dev` now.==='",
    "heroku-postbuild": "webpack --config=./config/webpack.prod.js",
    "dev": "PORT=3000 bundle exec foreman start --procfile=Procfile.dev",
    "prod": "PORT=3000 bundle exec foreman start --procfile=Procfile.prod",
    "webpack": "./node_modules/.bin/webpack-dev-server --config config/webpack.config.js --host 0.0.0.0",
    "test": "jest --coverage --no-cache",
    "typecheck": "tsc --noEmit --jsx preserve"
  },
  "keywords": [
    "farmbot"
  ],
  "author": "farmbot.io",
  "license": "MIT",
  "optionalDependencies": {
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.25.1",
    "@blueprintjs/datetime": "^1.20.0",
    "@blueprintjs/labs": "^0.4.0",
    "@types/deep-freeze": "^0.1.1",
    "@types/enzyme": "^2.8.6",
    "@types/fastclick": "^1.0.28",
    "@types/handlebars": "^4.0.35",
    "@types/history": "^4.6.0",
    "@types/i18next": "^2.3.32",
    "@types/jest": "^20.0.7",
    "@types/lodash": "^4.14.73",
    "@types/markdown-it": "^0.0.2",
    "@types/moxios": "^0.4.3",
    "@types/mqtt": "^0.0.34",
    "@types/node": "^8.0.24",
    "@types/react": "15.0.39",
    "@types/react-color": "^2.13.0",
    "@types/react-dom": "^15.5.3",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "3",
    "@types/redux": "^3.6.31",
    "axios": "^0.16.2",
    "boxed_value": "^1.0.0",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.5",
    "deep-freeze": "^0.0.1",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "farmbot": "4.3.9",
    "farmbot-toastr": "^1.0.3",
    "fastclick": "^1.0.6",
    "file-loader": "^0.11.2",
    "handlebars": "^4.0.10",
    "i18next": "^8.4.3",
    "imports-loader": "^0.7.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "markdown-it": "^8.3.2",
    "markdown-it-emoji": "^1.4.0",
    "moment": "^2.18.1",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-color": "^2.13.5",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^6.0.6",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.10",
    "ts-lint": "^4.5.1",
    "ts-loader": "^2.3.3",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-uglify-js-plugin": "^1.1.9",
    "weinre": "^2.0.0-pre-I0Z7U9OV",
    "which": "^1.3.0",
    "yarn": "^0.27.5"
  },
  "devDependencies": {
    "jscpd": "^0.6.13",
    "webpack-notifier": "^1.5.0"
  },
  "jest": {
    "globals": {
      "globalConfig": {
        "NODE_ENV": "development",
        "TOS_URL": "https://farmbot1.io/tos/",
        "LONG_REVISION": "------------",
        "SHORT_REVISION": "--------"
      }
    },
    "moduleNameMapper": {
      "^.*\\.scss$": "<rootDir>/webpack/__test_support__/stub.ts"
    },
    "setupFiles": [
      "./webpack/unmock_i18next.ts",
      "./webpack/__test_support__/localstorage.js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "webpack/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "json",
      "lcov"
    ]
  }
}
